
        <% for(var i = 0;i<cars.length;i++){%> 
      <div class="col-md-3">
        <div class="card ms-2 mt-4" style="width: 18rem;">
          <img src="<%= cars[i].image %> /img/image 1.png" class="card-img-top" alt="...">
          <div class="card-body ">
            <tr>
              <td><p class="card-title"><%= cars[i].name %></p></td><br>
              <td><p class="fw-bold">RP <%= cars[i].price %>/ hari </p> </td><br>
              <!-- <td><p class="me-2">Ukuran <%= cars[i].size %></p> </td><br> -->
              
              <td><p><i class="bi bi-clock me-2"></i>Update at:<%= cars[i].updatedAt  %></p> </td>
              <td>
                <div class=" row">
                  <div class="col-md-6 ">
                    <a class="btn btn-delete" id="delete_car" data-id=<%= cars[i]._id %>><i class="bi bi-trash me-2"></i>Delete</a>
                  </div>
                  <div class="col-md-6 ">
                    <a href="/update-car?id=<%= cars[i]._id %>" class="btn btn-edit"><i class="bi bi-pencil-square me-3"></i></i>Edit</a>
                  </div>
                </div>
              </td>
            </tr>
          </div>
        </div>
      </div>
    <% } %> 